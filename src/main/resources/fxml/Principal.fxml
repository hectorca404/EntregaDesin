    <children>
      <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="52.0" prefWidth="466.0">
         <children>
      
              <Button fx:id="regisBoton" text="Registrarse">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin></Button>
         </children>
         <VBox.margin>
            <Insets right="20.0" top="-10.0" />
         </VBox.margin>
      </HBox>
        <ImageView fx:id="logo" fitWidth="150" preserveRatio="true" rotate="90.0" />

        <Button fx:id="loginBoton" text="Iniciar Sesión" />

        <Label text="¿Has olvidado tu contraseña?" />
    </children>
</VBox>
